{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.js"
    }
  ],
  "functions": {
    "api/index.js": {
      "includeFiles": "backend/**"
    }
  },
  "installCommand": "cd backend && npm ci --only=production && npx prisma generate",
  "buildCommand": "mkdir -p dist",
  "outputDirectory": "dist",
  "framework": null
}