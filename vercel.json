{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.js"
    }
  ],
  "installCommand": "cd backend && npm ci --only=production && npx prisma generate",
  "buildCommand": "mkdir -p dist && echo 'Backend Online' > dist/index.html",
  "outputDirectory": "dist",
  "framework": null
}